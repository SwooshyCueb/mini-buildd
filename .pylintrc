[MESSAGES CONTROL]

disable=C0111,C0301,
        W0142,W0232,W0603,W0702,W0703,
        I0011,
        R0401,W0621

[TYPECHECK]
# see ./devel pylintgeneratedmembers
generated-members=add,all,build_keyring_package_for,codename,create_default_layout,DoesNotExist,filechroot,get_status_display,id,looplvmchroot,lvmchroot,mandatory_architectures,mbd_get_apt_line,mbd_get_apt_pin,mbd_get_archive,mbd_get_default_version,mbd_get_mandatory_version_regex,mbd_id,objects,split,splitlines,suites,admin_site,suiteoption_set,suite,layout,uploadable,get_union_type_display mbd_unprepare,architectureoption_set

[BASIC]

good-names=mini-buildd,mini-buildd-tool,urlpatterns,register
variable-rgx=[a-z_][a-z0-9_]{0,30}$
method-rgx=[a-z_][a-z0-9_]{2,40}$

[DESIGN]

# Django adds many public methods to classes, FTPDHandler is the winner though
max-public-methods=105
min-public-methods=0

# We need to pimp some default values a little
max-args=10
max-locals=30
max-branchs=25
max-attributes=15
max-statements=60

# Since 'python-debian (0.1.21+nmu2)': Changes: Too many ancestors (12/7)
max-parents=12

[REPORTS]

include-ids=yes
